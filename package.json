{
  "name": "kui",
  "version": "1.0.0",
  "packageManager": "^npm@9.0.0",
  "description": "A fun react ui component library! And React components implementation",
  "title": "Kui Design Library",
  "keywords": [
    "kui",
    "component",
    "components",
    "design",
